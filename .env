# Environment Configuration for Iron Doom Jarvis
# Copy this file to .env and fill in your API keys

# Discord Bot Configuration
DISCORD_TOKEN=your_discord_bot_token_here
PRIMARY_CHANNEL_ID=your_primary_channel_id_here

# Notion API Configuration - CONFIGURED!
NOTION_TOKEN=your_notion_token_here
NOTION_TASK_DATABASE_ID=your_notion_task_database_id_here
NOTION_NOTES_DATABASE_ID=your_notion_notes_database_id_here

# YouTube Data API
YOUTUBE_API_KEY=your_youtube_api_key_here

# Google Books API
GOOGLE_BOOKS_API_KEY=your_google_books_api_key_here

# News API
NEWS_API_KEY=your_news_api_key_here

# GitHub API
GITHUB_TOKEN=your_github_token_here
GITHUB_USERNAME=your_github_username_here

# OpenAI API (Optional for enhanced AI features)
OPENAI_API_KEY=your_openai_api_key_here

# Google Gemini API
GEMINI_API_KEY=your_gemini_api_key_here

# Logging Configuration
LOG_LEVEL=INFO

# Development Configuration
DEVELOPMENT=false